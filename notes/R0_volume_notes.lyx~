\begin_body

\begin_layout Standard
We want to determine the 
\begin_inset Quotes eld
\end_inset

volume
\begin_inset Quotes erd
\end_inset

 of parameter space for which 
\begin_inset Formula $\mathcal{R}_{0}>1$
\end_inset

 for biting rate (
\begin_inset Formula $b$
\end_inset

), mortality rate (
\begin_inset Formula $\mu_{V}$
\end_inset

) and competence (
\begin_inset Formula $\tau_{VH}$
\end_inset

).
 We assuming these values are sampled from independent normal distributions
 with means given by their baseline values and standard deviation 
\begin_inset Formula $\sigma$
\end_inset

 (the environmental noise strength), say 
\begin_inset Formula $b\sim\mathscr{N}\left(b^{*},\sigma\right)$
\end_inset

, 
\begin_inset Formula $\mu_{V}\sim\mathscr{N}\left(\mu_{V}^{*},\sigma\right)$
\end_inset

, and 
\begin_inset Formula $\tau_{VH}\sim\mathscr{N}\left(\tau_{VH}^{*},\sigma\right)$
\end_inset

.
 To understand the 
\begin_inset Quotes eld
\end_inset

probability
\begin_inset Quotes erd
\end_inset

 of 
\begin_inset Formula $\mathcal{R}_{0}>1$
\end_inset

 when these parameters lie within two standard deviations of their mean,
 we assume that 
\begin_inset Formula 
\begin{align*}
b & \in\left[b^{*}-2\sigma,b^{*}+2\sigma\right],\\
\mu_{V} & \in\left[\mu_{V}^{*}-2\sigma,\mu_{V}^{*}+2\sigma\right],\\
\tau_{VH} & \in\left[\tau_{VH}^{*}-2\sigma,\tau_{VH}^{*}+2\sigma\right],
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
The basic reproduction number is given by 
\begin_inset Formula $\mathcal{R}_{0}\left(b,\mu_{V},\tau_{VH}\right)=\sqrt{\frac{b^{2}\tau_{HV}\tau_{VH}N_{V}}{\mu_{V}\gamma_{H}N_{H}}}$
\end_inset

.
 Let 
\begin_inset Formula $S=\left\{ b,\mu_{V},\tau_{VH}|\mathcal{R}_{0}\left(b,\mu_{V},\tau_{VH}\right)>1\right\} $
\end_inset

 and 
\begin_inset Formula $X=\left\{ b,\mu_{V},\tau_{VH}\right\} $
\end_inset

.
 We thus compute
\begin_inset Formula 
\begin{align*}
V\left(\sigma\right) & ={\displaystyle \frac{\left|S\right|}{\left|X\right|}}\\
 & =\frac{{\displaystyle \iiintop_{\mathcal{R}_{0}>1}}dbd\mu_{V}d\tau_{VH}}{{\displaystyle \iiintop}dbd\mu_{V}d\tau_{VH}}
\end{align*}

\end_inset

We can re-write 
\begin_inset Formula $S$
\end_inset

 using the facts that 
\begin_inset Formula $b,\mu_{V},\tau_{VH}>0$
\end_inset

 and
\begin_inset Formula 
\begin{align*}
\sqrt{\frac{b^{2}\tau_{HV}\tau_{VH}N_{V}}{\mu_{V}\gamma_{H}N_{H}}} & >1\\
b\tau_{VH}^{1/2}\mu_{V}^{-1/2} & >C^{-1}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
0\le b^{*}-2\sigma\le b & \le b^{*}+2\sigma\\
0\le\mu_{V}^{*}-2\sigma\le\mu_{V} & \le\mu_{V}^{*}+2\sigma,\\
0\le\tau_{VH}^{*}-2\sigma\le\tau_{VH} & \le\tau_{VH}^{*}+2\sigma\le1
\end{align*}

\end_inset

where 
\begin_inset Formula $C=\sqrt{\frac{\tau_{HV}N_{V}}{\gamma_{H}N_{H}}}$
\end_inset

.Let 
\begin_inset Formula $\mu_{V}\in\left[\mu_{V}^{*}-2\sigma,\mu_{V}^{*}+2\sigma\right]$
\end_inset

 be fixed.
 Then, since 
\begin_inset Formula $\tau_{VH}\le\tau_{VH}^{*}+2\sigma\le1$
\end_inset

, we must have
\begin_inset Formula 
\[
b\mu_{V}^{-1/2}>C^{-1}\tau_{VH}^{-1/2}>C^{-1}\left(\tau_{VH}^{*}+2\sigma\right)^{-1/2}
\]

\end_inset

Hence 
\begin_inset Formula 
\[
b>C^{-1}\left(\tau_{VH}^{*}+2\sigma\right)^{-1/2}\mu_{V}^{1/2}
\]

\end_inset

Finally, 
\begin_inset Formula 
\begin{align*}
\tau_{VH} & >C^{-2}\mu_{V}b^{-2}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
V\left(\sigma\right) & ={\displaystyle \iiintop_{\mathcal{R}_{0}>1}}dbd\mu_{V}d\tau_{VH}\\
 & ={\displaystyle \intop_{\mu_{V}^{*}-2\sigma}^{\mu_{V}^{*}+2\sigma}}\left(\intop_{C^{-1}\left(\tau_{VH}^{*}+2\sigma\right)^{-1/2}\mu_{V}^{1/2}}^{b^{*}+2\sigma}\left(\intop_{C^{-2}\mu_{V}b^{-2}}^{\tau_{VH}^{*}+2\sigma}d\tau_{VH}\right)db\right)d\mu_{V}\\
 & ={\displaystyle \intop_{\mu_{V}^{*}-2\sigma}^{\mu_{V}^{*}+2\sigma}}\left(\intop_{C^{-1}\left(\tau_{VH}^{*}+2\sigma\right)^{-1/2}\mu_{V}^{1/2}}^{b^{*}+2\sigma}\left(\left(\tau_{VH}^{*}+2\sigma\right)-C^{-2}\mu_{V}b^{-2}\right)db\right)d\mu_{V}\\
 & =\intop_{\mu_{V}^{*}-2\sigma}^{\mu_{V}^{*}+2\sigma}\left(\tau_{VH}^{*}+2\sigma\right)\left(\left(b^{*}+2\sigma\right)-C^{-1}\left(\tau_{VH}^{*}+2\sigma\right)^{-1/2}\mu_{V}^{1/2}\right)+C^{-2}\mu_{V}\left(\left(b^{*}+2\sigma\right)^{-1}-\left(C^{-1}\left(\tau_{VH}^{*}+2\sigma\right)^{-1/2}\mu_{V}^{1/2}\right)^{-1}\right)d\mu_{V}\\
 & =\intop_{\mu_{V}^{*}-2\sigma}^{\mu_{V}^{*}+2\sigma}\left(b^{*}+2\sigma\right)\left(\tau_{VH}^{*}+2\sigma\right)-C^{-1}\left(\tau_{VH}^{*}+2\sigma\right)^{1/2}\mu_{V}^{1/2}+C^{-2}\left(b^{*}+2\sigma\right)^{-1}\mu_{V}-C^{-1}\left(\tau_{VH}^{*}+2\sigma\right)^{1/2}\mu_{V}^{1/2}d\mu_{V}\\
 & =\left(b^{*}+2\sigma\right)\left(\tau_{VH}^{*}+2\sigma\right)4\sigma-\frac{2}{3}C^{-1}\left(\tau_{VH}^{*}+2\sigma\right)^{1/2}\left(\left(\mu_{V}^{*}+2\sigma\right)^{3/2}-\left(\mu_{V}^{*}-2\sigma\right)^{3/2}\right)+4C^{-2}\left(b^{*}+2\sigma\right)^{-1}\sigma\mu_{V}^{*}-\frac{2}{3}C^{-1}\left(\tau_{VH}^{*}+2\sigma\right)^{1/2}\left(\left(\mu_{V}^{*}+2\sigma\right)^{3/2}-\left(\mu_{V}^{*}-2\sigma\right)^{3/2}\right)\\
\end{align*}

\end_inset


\end_layout

