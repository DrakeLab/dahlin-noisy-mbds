\end_layout

\begin_layout Standard
Let 
\begin_inset Formula $x=\ln b$
\end_inset

, 
\begin_inset Formula $y=\ln\sqrt{\tau_{VH}}$
\end_inset

, 
\begin_inset Formula $z=\ln\sqrt{\frac{1}{\mu_{V}}}$
\end_inset

 and 
\begin_inset Formula $R=\ln\mathcal{R}_{0}$
\end_inset

.
 Then 
\begin_inset Formula 
\begin{align*}
R & =\ln\left(b\sqrt{\frac{1}{\mu_{V}}}\tau_{VH}\frac{1}{c}\right)\\
 & =x+y+z-c
\end{align*}

\end_inset

and 
\begin_inset Formula $S=\left\{ x,y,z|R\left(x,y,z\right)>0\right\} $
\end_inset

.
 
\end_layout

\begin_layout Standard
asdf
\begin_inset Newline newline
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula 
\begin{align*}
V\left(\sigma\right) & ={\displaystyle \iiintop_{S}}\mathcal{R}_{0}\left(b,\mu_{V},\tau_{VH}\right)dbd\mu_{V}d\tau_{VH}\\
 & ={\displaystyle \iiintop_{S}}R\left(x,y,z\right)\frac{\partial\left(b,\tau_{VH},\mu_{V}\right)}{\partial\left(x,y,z\right)}dxdydz
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
c\sqrt{\frac{\mu_{V}}{\tau_{VH}}}<b & \le b^{*}+2\sigma\\
b\sqrt{\tau_{VH}}\sqrt{\frac{1}{\mu_{V}}} & >c\\
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
V\left(\sigma\right) & =\frac{1}{c}\intop_{\mu_{V}^{*}-2\sigma}^{\mu_{V}^{*}+2\sigma}\left(\intop_{\mu_{V}\left(\frac{c}{b}\right)^{2}}^{\tau_{VH}^{*}+2\sigma}\left(\intop_{c\sqrt{\frac{\mu_{V}}{\tau_{VH}}}}^{b^{*}+2\sigma}b\sqrt{\frac{\tau_{VH}}{\mu_{V}}}db\right)d\tau_{VH}\right)d\mu_{V}\\
 & =\frac{1}{c}\intop_{\mu_{V}^{*}-2\sigma}^{\mu_{V}^{*}+2\sigma}\left(\intop_{\mu_{V}\left(\frac{c}{b}\right)^{2}}^{\tau_{VH}^{*}+2\sigma}\left(\frac{1}{2}\sqrt{\frac{\tau_{VH}}{\mu_{V}}}\left[\left(b^{*}+2\sigma\right)^{2}-\left(c\sqrt{\frac{\mu_{V}}{\tau_{VH}}}\right)^{2}\right]\right)d\tau_{VH}\right)d\mu_{V}\\
 & =\frac{1}{2c}\intop_{\mu_{V}^{*}-2\sigma}^{\mu_{V}^{*}+2\sigma}\left(\intop_{\mu_{V}\left(\frac{c}{b}\right)^{2}}^{\tau_{VH}^{*}+2\sigma}\left(b^{*}+2\sigma\right)^{2}\sqrt{\frac{1}{\mu_{V}}}\sqrt{\tau_{VH}}-c^{2}\sqrt{\mu_{V}}\sqrt{\frac{1}{\tau_{VH}}}d\tau_{VH}\right)d\mu_{V}\\
 & =\frac{1}{2c}\intop_{\mu_{V}^{*}-2\sigma}^{\mu_{V}^{*}+2\sigma}\frac{2}{3}\left(b^{*}+2\sigma\right)^{2}\sqrt{\frac{1}{\mu_{V}}}\left[\left(\tau_{VH}^{*}+2\sigma\right)^{3/2}-\left[\mu_{V}\left(\frac{c}{b}\right)^{2}\right]^{3/2}\right]-2c^{2}\sqrt{\mu_{V}}\left[\left(\tau_{VH}^{*}+2\sigma\right)^{1/2}-\left(\mu_{V}\left(\frac{c}{b}\right)^{2}\right)^{1/2}\right]d\mu_{V}\\
 & =\frac{1}{c}\intop_{\mu_{V}^{*}-2\sigma}^{\mu_{V}^{*}+2\sigma}\frac{1}{3}\left(b^{*}+2\sigma\right)^{2}\left[\left(\tau_{VH}^{*}+2\sigma\right)^{3/2}\mu_{V}^{-1/2}-\left(\frac{c}{b}\right)^{3}\mu_{V}^{3/2}\right]-c^{2}\left[\left(\tau_{VH}^{*}+2\sigma\right)^{1/2}\mu_{V}^{-1/2}-\left(\frac{c}{b}\right)\right]d\mu_{V}\\
 & =\frac{1}{c}\left\{ \frac{2}{3}\left(b^{*}+2\sigma\right)^{2}\left[\left(\tau_{VH}^{*}+2\sigma\right)^{3/2}\left[\left(\mu_{V}^{*}+2\sigma\right)^{1/2}-\left(\mu_{V}^{*}-2\sigma\right)^{1/2}\right]-\frac{1}{5}\left(\frac{c}{b}\right)^{3}\left[\left(\mu_{V}^{*}+2\sigma\right)^{5/2}-\left(\mu_{V}^{*}-2\sigma\right)^{5/2}\right]\right]-c^{2}\left[2\left(\tau_{VH}^{*}+2\sigma\right)^{1/2}\left[\left(\mu_{V}^{*}+2\sigma\right)^{1/2}-\left(\mu_{V}^{*}-2\sigma\right)^{1/2}\right]-\left(\frac{c}{b}\right)\left[\mu_{V}^{*}+2\sigma-\mu_{V}^{*}+2\sigma\right]\right]\right\} \\
 & =\frac{1}{c}\left\{ \frac{2}{3}\left(b^{*}+2\sigma\right)^{2}\left[\left(\tau_{VH}^{*}+2\sigma\right)^{3/2}\left[\left(\mu_{V}^{*}+2\sigma\right)^{1/2}-\left(\mu_{V}^{*}-2\sigma\right)^{1/2}\right]-\frac{1}{5}\left(\frac{c}{b}\right)^{3}\left[\left(\mu_{V}^{*}+2\sigma\right)^{5/2}-\left(\mu_{V}^{*}-2\sigma\right)^{5/2}\right]\right]-c^{2}\left[2\left(\tau_{VH}^{*}+2\sigma\right)^{1/2}\left[\left(\mu_{V}^{*}+2\sigma\right)^{1/2}-\left(\mu_{V}^{*}-2\sigma\right)^{1/2}\right]-4\left(\frac{c}{b}\right)\sigma\right]\right\} 
\end{align*}

\end_inset


\end_layout

